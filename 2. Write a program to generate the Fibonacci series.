nterms = 12
n1, n2 = 0, 1
count = 2

if nterms <= 0:
    print("Please enter a positive integer")
elif nterms == 1:
    print("Fibonacci sequence up to", nterms, ":")
    print(n1)
else:
    print("Fibonacci sequence up to", nterms, ":")
    print(n1, ",", n2, end=", ")
    while count < nterms:
        nth = n1 + n2
        print(nth, end=", ")
        n1, n2 = n2, nth
        count += 1
