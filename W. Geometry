
import math


def squareArea(side):
    return side ** 2

def circleArea(radius):
    return math.pi * radius ** 2


def pointyShapeVolume(x, h, square):
    if square:
        base = squareArea(x)
    else:
        base = circleArea(x)
    return h * base / 3.0

print(dir())
print(pointyShapeVolume(4, 2.6, True))
print(pointyShapeVolume(4, 2.6, False))
